{
  "defaults": {
    "uvlock": true
  },
  "multipart": [
    {
      "when": { "north": true },
      "apply": { "model": "yaass:block_cable_connected_side" }
    },
    {
      "when": { "north": false },
      "apply": { "model": "yaass:block_cable_side" }
    },
    
    {
      "when": { "south": true },
      "apply": { "model": "yaass:block_cable_connected_side", "y": 180 }
    },
    {
      "when": { "south": false },
      "apply": { "model": "yaass:block_cable_side", "y": 180 }
    },
    
    {
      "when": { "east": true },
      "apply": { "model": "yaass:block_cable_connected_side", "y": 90 }
    },
    {
      "when": { "east": false },
      "apply": { "model": "yaass:block_cable_side", "y": 90 }
    },
    
    {
      "when": { "west": true },
      "apply": { "model": "yaass:block_cable_connected_side", "y": 270 }
    },
    {
      "when": { "west": false },
      "apply": { "model": "yaass:block_cable_side", "y": 270 }
    },
    
    {
      "when": { "up": true },
      "apply": { "model": "yaass:block_cable_connected_side", "x": 270 }
    },
    {
      "when": { "up": false },
      "apply": { "model": "yaass:block_cable_side", "x": 270 }
    },
    
    {
      "when": { "down": true },
      "apply": { "model": "yaass:block_cable_connected_side", "x": 90 }
    },
    {
      "when": { "down": false },
      "apply": { "model": "yaass:block_cable_side", "x": 90 }
    }
  ]
}
